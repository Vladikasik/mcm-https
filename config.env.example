# Memory MCP Server Configuration
# Copy this file to .env and update the values

# Environment mode (development/production)
ENV=production

# Server configuration
HOST=0.0.0.0
PORT=8443

# SSL Certificate Configuration (Production)
# Update these paths to your actual certificate files for memory.aynshteyn.dev
SSL_CERTFILE=/home/aynshteyn.dev-ssl-bundle/domain.cert.pem
SSL_KEYFILE=/home/aynshteyn.dev-ssl-bundle/private.key.pem

# Server name
SERVER_NAME=MemoryMCP

# Optional: Log level
LOG_LEVEL=info

# Domain configuration
DOMAIN=memory.aynshteyn.dev

# Optional: Number of workers
WORKERS=1 